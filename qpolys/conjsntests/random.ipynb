   "source": [
    "import numpy as np\n",
    "\n",
    "p = [1, 2, 3, 4]\n",
    "q = [-1, 2, -2, 1]\n",
    "r = 7\n",
    "\n",
    "def f(p, q):\n",
    "    return np.convolve(p, list(reversed(q)))\n",
    "\n",
    "def pf(p, q, r):\n",
    "    return np.convolve(p + [0]*(r- len(p)), [0]*(r- len(q)) + list(reversed(q)))\n",
    "\n",
    "[f(p, q), pf(p, q, r)]"
   ]
